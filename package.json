{
  "name": "vuemmerce",
  "version": "0.9.2",
  "description": "Responsive ecommerce template built with Vue.js and Nuxt.js",
  "author": "ivan lori <ivan.lori@protonmail.com>",
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "precommit": "npm run lint",
    "heroku-postbuild": "npm run build"
  },
  "fix-absolute": "sed -i '/  .basedir.: /d' .nuxt/dist/server-bundle.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/ivanlori/Vuemmerce.git"
  },
  "keywords": [
    "css",
    "js",
    "html",
    "template",
    "ecommerce",
    "vuejs",
    "theme",
    "responsive",
    "nuxtjs"
  ],
  "license": "MIT",
  "dependencies": {
    "@nuxtjs/font-awesome": "^1.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bulma": "^0.7.5",
    "event-stream": "^4.0.1",
    "nuxt": "^2.15.8",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "babel-plugin-transform-runtime": "^6.23.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.2.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
